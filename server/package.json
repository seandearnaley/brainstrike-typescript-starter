{
  "name": "brainstrike-server",
  "version": "1.0.0",
  "description": "brainkstrike typescript node API",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "yarn run build && node build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "coverage": "jest --coverage",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:dev": "graphql-codegen --config codegen.yml --watch",
    "dev": "concurrently \"yarn run start:dev\" \"yarn run generate:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seandearnaley/brainstrike-server.git"
  },
  "keywords": [
    "api",
    "node",
    "apollo",
    "typescript"
  ],
  "author": "Sean Dearnaley <seandearnaley@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seandearnaley/brainstrike-server/issues"
  },
  "homepage": "https://github.com/seandearnaley/brainstrike-server#readme",
  "resolutions": {
    "@graphql-codegen/**/mem": "^6.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.1",
    "@graphql-codegen/typescript": "1.9.1",
    "@graphql-codegen/typescript-resolvers": "1.9.1",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "apollo-link": "^1.2.13",
    "apollo-link-http": "^1.5.16",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "nock": "^11.7.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/graphql-iso-date": "^3.3.3",
    "@types/node-fetch": "^2.5.4",
    "apollo-server-express": "^2.9.15",
    "apollo-server-testing": "^2.9.15",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-import": "^0.7.1",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.15",
    "pg": "^7.17.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.22"
  }
}
