{
  "name": "brainstrike-server",
  "version": "1.0.0",
  "description": "brainkstrike typescript node API",
  "main": "build/index.js",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "start:dev": "nodemon",
    "start": "yarn run build && node build/index.js",
    "test": "yarn run build && jest --verbose",
    "test:watch": "yarn run build && jest --watch",
    "lint": "eslint './src/**/*.{ts,tsx}' --fix",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "create-fake-content": "yarn run build && node build/utility/faker/createFakeContent.js",
    "coverage": "jest --coverage",
    "generate": "graphql-codegen --config codegen.ts",
    "generate:dev": "graphql-codegen --config codegen.ts --watch",
    "generate:auto": "node generate-code.mjs",
    "dev": "concurrently \"yarn run start:dev\" \"yarn run generate:dev\"",
    "migrate:up": "yarn run build && NODE_ENV=migration node build/migrate.js up",
    "migrate:down": "yarn run build && NODE_ENV=migration node build/migrate.js down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seandearnaley/brainstrike-server.git"
  },
  "keywords": [
    "api",
    "node",
    "apollo",
    "typescript"
  ],
  "author": "Sean Dearnaley <seandearnaley@hotmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seandearnaley/brainstrike-server/issues"
  },
  "homepage": "https://github.com/seandearnaley/brainstrike-server#readme",
  "resolutions": {
    "@graphql-codegen/**/mem": "6.0.1",
    "@types/ws": "8.5.4"
  },
  "devDependencies": {
    "@eslint/js": "9.21.0",
    "@graphql-codegen/cli": "5.0.5",
    "@graphql-codegen/typescript": "4.1.5",
    "@graphql-codegen/typescript-resolvers": "4.4.4",
    "@types/express-serve-static-core": "5.0.6",
    "@types/graphql-iso-date": "3.4.3",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.16",
    "@types/node": "22.13.7",
    "@types/node-fetch": "2.6.12",
    "@typescript-eslint/eslint-plugin": "8.25.0",
    "@typescript-eslint/parser": "8.25.0",
    "apollo-link": "1.2.14",
    "apollo-link-http": "1.5.17",
    "eslint": "9.21.0",
    "eslint-config-prettier": "10.0.2",
    "eslint-import-resolver-typescript": "3.8.3",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "jest": "29.7.0",
    "nock": "14.0.1",
    "nodemon": "3.1.9",
    "prettier": "3.5.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.25.0"
  },
  "dependencies": {
    "@apollo/server": "4.10.2",
    "@faker-js/faker": "8.4.1",
    "@graphql-tools/schema": "10.0.3",
    "apollo-server-express": "3.13.0",
    "concurrently": "9.1.2",
    "dataloader": "2.2.3",
    "dotenv": "16.4.7",
    "express": "4.21.2",
    "graphql": "16.10.0",
    "graphql-import": "1.0.2",
    "graphql-iso-date": "3.6.1",
    "graphql-tag": "2.12.6",
    "lodash": "4.17.21",
    "node-fetch": "2.6.12",
    "npm-check-updates": "17.1.15",
    "opaqueid": "1.2.0",
    "patch-package": "8.0.0",
    "pg": "8.13.3",
    "postinstall-postinstall": "2.1.0",
    "reflect-metadata": "0.2.2",
    "typeorm": "0.3.20"
  },
  "packageManager": "yarn@3.6.0"
}
