{
  "name": "brainstrike-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --verbose --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "lint": "eslint './src/**/*.{ts,tsx}'",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "generate": "graphql-codegen --config codegen.yml",
    "generate:dev": "graphql-codegen --config codegen.yml --watch",
    "apollo:schema:download": "apollo schema:download --endpoint=http://localhost:4000/graphql graphql.schema.json",
    "dev": "concurrently \"yarn run start\" \"yarn run generate:dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@graphql-codegen/**/mem": "^6.0.1",
    "jest-environment-jsdom": "^26.0.1"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@types/jest": "26.0.10",
    "@types/node": "14.6.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-test-renderer": "^16.9.3",
    "@types/yup": "^0.29.6",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-utilities": "^1.3.4",
    "concurrently": "^5.3.0",
    "date-fns": "^2.15.0",
    "emotion": "^10.0.27",
    "formik": "^2.1.5",
    "formik-material-ui": "^2.0.1",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "opaqueid": "^1.2.0",
    "react": "^16.13.1",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-test-renderer": "^16.13.1",
    "typescript": "3.9.7",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "2.4.0",
    "@graphql-codegen/typescript-react-apollo": "3.2.14",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.9",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "apollo": "^2.30.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2"
  }
}
