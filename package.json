{
  "name": "brainstrike-typescript-starter",
  "version": "1.0.0",
  "description": "Starter kit for Node.js + React.js + Apollo GraphQL + TypeORM",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "cd client && yarn install",
    "server-install": "cd server && yarn install",
    "install-all-deps": "concurrently \"yarn run server-install\" \"yarn run client-install\"",
    "client-test": "cd client && yarn run test",
    "server-test": "cd server && yarn run test",
    "all-tests": "concurrently \"yarn run server-test\" \"yarn run client-test\"",
    "client-start": "cd client && yarn run start",
    "server-start": "cd server && yarn run start:dev",
    "all-start": "concurrently \"yarn run server-start\" \"yarn run client-start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seandearnaley/brainstrike-typescript-starter.git"
  },
  "keywords": [
    "Node",
    "React",
    "Apollo",
    "GraphQL",
    "TypeORM",
    "Prettier",
    "Material-UI",
    "Jest",
    "ESLint"
  ],
  "author": "Sean Dearnaley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seandearnaley/brainstrike-typescript-starter/issues"
  },
  "homepage": "https://github.com/seandearnaley/brainstrike-typescript-starter#readme",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "concurrently": "^5.3.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0"
  }
}
